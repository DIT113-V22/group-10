<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior">

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="4dp"
        android:onClick="openSettings"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_baseline_settings_24" />

    <Button
        android:id="@+id/ShoppingB"
        android:layout_width="170dp"
        android:layout_height="170dp"
        android:layout_marginTop="90dp"
        android:backgroundTint="#D5C7C1"
        android:drawableTop="@drawable/stall"
        android:text="Shope-in-Shope"
        android:textColor="@color/black"
        app:cornerRadius="20sp"
        app:layout_constraintEnd_toStartOf="@+id/allDeliveriesB"
        app:layout_constraintHorizontal_bias="0.568"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView18" />

    <Button
        android:id="@+id/allDeliveriesB"
        android:layout_width="170dp"
        android:layout_height="170dp"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="20dp"
        android:backgroundTint="#F0EFEF"
        android:drawableTop="@drawable/deliveryhome"
        android:text="All deliveries"
        android:textColor="@color/black"
        app:cornerRadius="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView18" />

    <Button
        android:id="@+id/button10"
        android:layout_width="170dp"
        android:layout_height="170dp"
        android:layout_marginTop="36dp"
        android:backgroundTint="#D3D4DB"
        android:drawableTop="@drawable/feedback"
        android:text="Feedback"
        android:textColor="@color/black"
        app:cornerRadius="20sp"
        app:layout_constraintEnd_toStartOf="@+id/termsB"
        app:layout_constraintHorizontal_bias="0.562"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ShoppingB" />

    <Button
        android:id="@+id/termsB"
        android:layout_width="170dp"
        android:layout_height="170dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="20dp"
        android:backgroundTint="#DFDACB"
        android:drawableTop="@drawable/termsandconditions"
        android:text="Terms and Conditions"
        android:textColor="@color/black"
        app:cornerRadius="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/allDeliveriesB" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="340dp"
        android:layout_height="45dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="55dp"
        android:text="Latest Delivery"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView19" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="323dp"
        android:layout_height="60dp"
        android:layout_marginStart="7dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="30dp"
        android:text="Welcome,"
        android:textColor="@color/black"
        android:textSize="40sp"
        app:layout_constraintEnd_toStartOf="@+id/imageButton"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="442dp"
        android:layout_height="574dp"
        android:layout_marginTop="87dp"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        app:srcCompat="@drawable/hermes_lego_no_background_small_reverse"
        tools:layout_editor_absoluteX="-7dp" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="340dp"
        android:layout_height="45dp"
        android:layout_marginTop="130dp"
        android:layout_marginBottom="7dp"
        android:text="Activities"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/ShoppingB"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        tools:layout_editor_absoluteX="16dp" />

</androidx.constraintlayout.widget.ConstraintLayout>